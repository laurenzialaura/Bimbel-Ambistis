batang <- function(a){
sisa <- a%%6
hasil.bagi <- as.integer(a/6)
print(paste("Emas 6 kg yang dipakai = ", hasil.bagi, " batang"))
print(paste("Emas 1 kg yang dipakai = ", sisa, " batang"))
print(paste("Jumlah batang emas yang dipakai = ", sisa + hasil.bagi , " batang"))
return(sisa+hasil.bagi)
}
batang(15)
batang(7)
batang(22)
